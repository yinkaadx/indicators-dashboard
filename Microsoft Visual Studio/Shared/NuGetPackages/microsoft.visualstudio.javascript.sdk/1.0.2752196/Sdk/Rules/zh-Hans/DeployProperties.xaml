<Rule Name="DeployProperties" PageTemplate="generic" DisplayName="部署" Description="部署选项" xmlns="http://schemas.microsoft.com/build/2009/properties">

    <Rule.DataSource>
        <DataSource Persistence="ProjectFile" HasConfigurationCondition="False"/>
